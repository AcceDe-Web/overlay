{
  "title": "AcceDe Web - modal",
  "name": "accedeweb-modal",
  "version": "0.0.0",
  "description": "WAI-ARIA modal plugin based on AcceDe Web instructions",
  "main": "dist/modal.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "test": "tape test/*.js | faucet",
    "build": "rollup -c && rollup -c rollup.config.minified.js",
    "doc": "documentation build lib/index.js -f html -o docs --shallow",
    "doc-lint": "documentation lint lib/index.js",
    "doc-serve": "documentation serve",
    "version": "npm run test && npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "server": "browser-sync start -s 'test' --ss 'dist' -f 'test' 'dist' --no-open --no-ui --no-ghost-mode --no-notify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AcceDe-Web/modal.git"
  },
  "keywords": [
    "accessibility",
    "modal",
    "popin",
    "dialog",
    "javascript",
    "vanilla"
  ],
  "author": {
    "name": "Switch",
    "email": "bonjour@switch.paris",
    "url": "http://switch.paris"
  },
  "license": "ISC",
  "homepage": "http://a11y.switch.paris/",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.13",
    "documentation": "^5.3.1",
    "faucet": "0.0.1",
    "nightmare": "^2.10.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tape": "^4.8.0"
  }
}
